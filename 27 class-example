<body>
  <h1>Class Example</h1>
  <script>
    class person {
      constructor(name, age, loc) {
        this.name = name;
        this.age = age;
        this.loc = loc;
      }
      pname() {
        console.log(this.name + " " + this.age);
        return this.age;
      }
    }
    const p1 = new person("Ayush", 18, "Mumbai");
    console.log(p1.pname());
  </script>
</body>
