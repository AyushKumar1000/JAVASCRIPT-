<!DOCTYPE html>
<html>
<head>
    <title>Array Operations</title>
</head>
<body>
    <script>
        let arr = [1, 2, 3, 4, 5];
        let arr2 = ['a', 'b', 'c'];
        let arr3 = [1, 2, 3];

        console.log("concat:", arr2.concat(arr3));
        console.log("join:", arr2.join('-'));
        console.log("includes:", arr.includes(3));
        console.log("indexOf:", arr.indexOf(3));
        console.log("lastIndexOf:", [1, 2, 3, 2, 1].lastIndexOf(2));
        let rev = [...arr];
        console.log("reverse:", rev.reverse());
        let nums = [4, 2, 5, 1, 3];
        console.log("sort:", nums.sort((a, b) => a - b));
    </script>
</body>
</html>
